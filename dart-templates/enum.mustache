      case {{{value}}}:
        value = data;
        break;
      {{/enumVars}}{{/allowableValues}}
      default:
        throw('Unknown enum value to decode: $data');
    }
  }

  {{classname}}._internal(this.value);

  /// The underlying value of this enum member.
